<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/saved_act"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/map"
    tools:context=".SavedNotificationsActivity">

    <TableRow
        android:id="@+id/tableRow"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:background="@drawable/saved_upper"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/icon_white"
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            app:layout_constraintHorizontal_bias="0.496"


            />

        <ImageButton
            android:id="@+id/back_btn"
            android:layout_width="35dp"
            android:layout_height="51dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/back" />


    </TableRow>

    <ImageView
        android:id="@+id/choice_table"
        android:layout_width="289dp"
        android:layout_height="559dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="60dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tableRow"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/table" />

    <ScrollView
        android:id="@+id/ScrollTable"
        android:layout_width="299dp"
        android:layout_height="361dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="@+id/choice_table"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/choice_table"
        app:layout_constraintTop_toTopOf="@+id/choice_table">

        <TableLayout
            android:id="@+id/notificationsList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>